{
  "plugins": ["plugins/markdown"],
  "recurseDepth" : 10,
  "source": {
    "include": "dist",
    "incluePattern": ".js$",
    "excludePattern": "(node_modules|docs)"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true
  }
}